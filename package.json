{
  "name": "decode-js",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "decode": "node src/main.js -i input.js -o output/output.js",
    "second": "node second-pass.js output/output.js output/output.deob2.js",
    "decode:all": "npm run decode && npm run second",
    "deob": "node src/main.js -t obfuscator -i input.js -o output/output.js",
    "deso": "node src/main.js -t sojson -i input.js -o output/output.js",
    "desov7": "node src/main.js -t sojsonv7 -i input.js -o output/output.js",
    "lint": "eslint --ext .js --fix src"
  },
  "dependencies": {
    "@babel/core": "^7.25.0",
    "@babel/parser": "^7.25.0",
    "@babel/traverse": "^7.25.0",
    "@babel/generator": "^7.25.0",
    "@babel/types": "^7.25.0",
    "base64url": "^3.0.1",
    "crypto-js": "^4.2.0",
    "isolated-vm": "^4.7.2",
    "prettier": "^2.7.1",
    "vm2": "^3.9.11"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1"
  }
}